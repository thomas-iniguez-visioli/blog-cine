  <a href="<%- config.root %><%- item.path %>" class="post">
    <div class="thumb" ><img src="https://thomas-iniguez-visioli.github.io/cdn/<%- item.featured_image%>" style=" object-fit: fill"></img>  </div>
    <article>
      <h1>
        <%=item.title %>
      </h1>
  
      <p>
      
      </p>
  
      <span>
        <%= item.date.format(config.date_format) %></span>
    </article>
  </a>

</div>